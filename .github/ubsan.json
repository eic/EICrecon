{
    "problemMatcher": [
        {
            "owner": "ubsan",
            "pattern": [
                {
                    "regexp": "^.*/(src/.+):(\\d+):(\\d+):\\sruntime\\s(error):\\s*(.*)$",
                    "file": 1,
                    "fromPath": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                },
            ]
        }
    ]
}
