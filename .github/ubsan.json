{
    "problemMatcher": [
        {
            "owner": "ubsan",
            "pattern": [
                {
                    "regexp": "^SUMMARY: UndefinedBehaviorSanitizer: undefined-behavior (.+):(\\d+):(\\d+) in\\s*$",
                    "file": 1,
                    "line": 2,
                    "column": 3
                },
                {
                    "regexp": "^(.+):(\\d+):(\\d+):\\s\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4,
                    "loop": false
                }
            ]
        }
    ]
}
